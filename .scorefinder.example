# Google API Configuration
GEMINI_API_KEY=your_gemini_api_key_here # get from console.google.com
GOOGLE_SEARCH_API_KEY=your_search_api_key_here # get from console.google.com
GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id_here # get from console.google.com
LLM_MODEL=gemini-2.5-flash # gemini-2.5-flash is most economical, gemini-2.5-pro will incur higher charges

# Application Configuration
# If left commented, subfolders in $SFHOME will be used
# OUTPUT_DIR=./scores
# TEMP_DIR=./temp
LOG_LEVEL=INFO # INFO or DEBUG
MAXIMUM_SEARCH_RESULTS=10 # a number between 1 and 10
MINIMUM_MEASURES=5 # a positive integer
SAVE_INTERMEDIATE=false # set true to download all source files
